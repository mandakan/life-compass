    "@radix-ui/react-switch": "^1.1.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
