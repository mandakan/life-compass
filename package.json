    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-unused-imports": "latest",
    "i18next-parser": "latest",
    "jsdom": "latest",
    "knip": "latest",
    "playwright": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "tailwindcss": "latest",
    "ts-prune": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "latest",
    "vitest": "latest"
  },
  "engines": {
    "node": "22.x"
  }
